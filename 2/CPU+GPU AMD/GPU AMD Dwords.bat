@echo off
:: Ensure admin privileges
fltmc >nul 2>&1 || (
    echo Administrator privileges are required.
    PowerShell Start -Verb RunAs '%0' 2> nul || (
        echo Right-click on the script and select "Run as administrator".
        pause & exit 1
    )
    exit 0
)
:: Initialize environment
setlocal EnableExtensions EnableDelayedExpansion

echo ======================================================
echo    Advanced AMD GPU Series-Specific Optimization Tool
echo ======================================================
echo.

:: Detect AMD GPU and determine series
echo Detecting AMD GPU series...
set "GPU_DETECTED=0"
set "SERIES="

for /f "tokens=*" %%i in ('wmic path win32_VideoController get Description ^| findstr /i "AMD Radeon"') do (
    set GPU_NAME=%%i
    set GPU_DETECTED=1
    
    echo Detected: !GPU_NAME!
    
    echo !GPU_NAME! | findstr /i "RX 5[0-9][0-9]" > nul
    if !errorlevel! equ 0 (
        set SERIES=RDNA1
        echo GPU Architecture: RDNA 1 (RX 5000 Series)
    )
    
    echo !GPU_NAME! | findstr /i "RX 6[0-9][0-9][0-9]" > nul
    if !errorlevel! equ 0 (
        set SERIES=RDNA2
        echo GPU Architecture: RDNA 2 (RX 6000 Series)
    )
    
    echo !GPU_NAME! | findstr /i "RX 7[0-9][0-9][0-9]" > nul
    if !errorlevel! equ 0 (
        set SERIES=RDNA3
        echo GPU Architecture: RDNA 3 (RX 7000 Series)
    )
    
    echo !GPU_NAME! | findstr /i "Vega" > nul
    if !errorlevel! equ 0 (
        set SERIES=VEGA
        echo GPU Architecture: Vega
    )
    
    echo !GPU_NAME! | findstr /i "RX 4[0-9][0-9]" > nul
    if !errorlevel! equ 0 (
        set SERIES=POLARIS
        echo GPU Architecture: Polaris (RX 400/500 Series)
    )
)

if %GPU_DETECTED% equ 0 (
    echo No AMD GPU detected. Optimization cannot proceed.
    pause
    exit /b 1
)

